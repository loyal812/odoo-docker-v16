<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <!-- stock.delivery.note inherit form view -->
  <record id="stock_delivery_note_carrier_inherit" model="ir.ui.view">
    <field name="name">stock.delivery.note.inherit.view.form</field>
    <field name="model">stock.delivery.note</field>
    <field name="inherit_id" ref="l10n_it_delivery_note.stock_delivery_note_form_view"/>
    <field name="arch" type="xml">
      <xpath expr="//field[@name='carrier_id']" position="replace">
        <field name="carrier_partner_id"/>
        
      </xpath>
      
    </field>
  </record>

</odoo>
